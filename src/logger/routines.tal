(
@|logger/routines )

@<set-error> ( name* -- )
	pstr: \fg-2 \0
	;Error pstr/ pstr/
	<\s>
	;token/buf pstr/
	;Info/in pstr/
	;scope pstr/
	[ LIT2 ". -Console/write ] DEO
	#01 ;abort STA
	pstr: \n \r \bg-0 \fg-1 \0
	JMP2r

@<set-warning> ( name* -- )
	pstr: \fg-2 \0
	;Warning pstr/ pstr/
	<\s>
	;token/buf pstr/
	;Info/in pstr/
	;scope pstr/
	[ LIT2 ". -Console/write ] DEO
	pstr: \n \r \bg-0 \fg-1 \0
	JMP2r
